            android:name="com.edf_decoha.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.edf_decoha.AfficheListeClient"
            android:label="@string/title_activity_affiche_client" >
