        <activity
            android:name="com.edf_decoha.ModifClient"
            android:label="@string/title_activity_modif_client" >
        </activity>
        <activity
            android:name="com.edf_decoha.Geoloc"
            android:label="@string/title_activity_geoloc" >
        </activity>
    </application>

</manifest>
